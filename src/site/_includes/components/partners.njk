<ul class="logo-list">
  {% for p in partners %}
    <li>
      <div class="aspect-ratio aspect-ratio-1/2">
        <a class="tpp-logo w-full h-full"
           title="{{p.name}}"
           href="{{p.url}}"
           target="_blank">
          <img class="tpp-logo__img" width="120" src="{{p.logoPath | url}}" alt="Logo of {{p.name}}">
        </a>
      </div>
    </li>
  {% endfor %}
</ul>
